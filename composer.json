{
    "name": "zuhaili92/dbcart",
    "description": "Shopping Cart library for Laravel 7 that uses Database instead of Sessions",
    "keywords": ["laravel", "cart", "database", "laravel7", "shopping-cart", "ecommerce"],
    "homepage": "https://github.com/zuhaili92/dbcart",
    "license": "MIT",
    "authors": [
        {
            "name": "zuhaili92",
            "email": "zuhaili.xperia@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "illuminate/support": "^7.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~0.9",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "zuhaili92\\DBCart\\": "src/DBCart"
        }
    },
    "minimum-stability": "dev",
    "extra":{
      "laravel":{
         "providers":[
            "zuhaili92\\DBCart\\CartServiceProvider"
         ]
      }
   }
}
